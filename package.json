{
  "name": "housing-calculator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "axios": "^0.19.0",
    "node-sass": "^7.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "test": "react-scripts test",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "precommit": "lint-staged",
    "verify": "run-s build lint test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint --fix",
      "git add"
    ],
    "*.scss": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  }
}
